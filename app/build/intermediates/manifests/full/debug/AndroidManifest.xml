<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.comli.dawnbreaksthrough.personalintro"
    android:versionCode="1"
    android:versionName="0.06" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="24" />

    <uses-feature
        android:name="android.hardware.camera2"
        android:required="false" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.comli.dawnbreaksthrough.personalintro.StartActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.comli.dawnbreaksthrough.personalintro.ViewPagerActivity"
            android:parentActivityName="com.comli.dawnbreaksthrough.personalintro.StartActivity" >
        </activity>
        <activity android:name="com.comli.dawnbreaksthrough.personalintro.SettingActivity" >
        </activity>
        <activity android:name="com.comli.dawnbreaksthrough.personalintro.LicenseActivity" >
        </activity>
    </application>

</manifest>